{
    "compilerOptions": {
        "target": "es2016",
        "module": "ESNext",
        "rootDir": "./",
        "baseUrl": "./src",
        "outDir": "./dist",
        "moduleResolution": "Bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["template"]
}
